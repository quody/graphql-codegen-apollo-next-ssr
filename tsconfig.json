{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es6",
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "preserve",
    "outDir": "build",
    "esModuleInterop": true
  },
  "exclude": [
    "build",
    "node_modules",
    "node_modules/**",
    "example",
  ]
}